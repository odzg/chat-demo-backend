{
  "$schema": "https://unpkg.com/knip@5/schema-jsonc.json",
  "entry": [
    // Type declaration files
    "@types/**/*.ts",
  ],
  "ignoreDependencies": [
    // Type-definitions for `ws`, which `@fastify/websocket` implicitly depends on
    "@types/ws",
  ],
  "treatConfigHintsAsErrors": true,
}
